export EDITOR=vim
export FPATH="/opt/homebrew/share/zsh/site-functions:$FPATH"
export LC_ALL=en_US.UTF-8
export LESS='-F -R'
export PAGER='less -F -R'
export PATH="/opt/homebrew/bin:/opt/homebrew/sbin:$HOME/bin:$HOME/.local/bin:$PATH"

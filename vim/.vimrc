syntax on
set ts=4
set sw=4
set sts=4
set expandtab
set showmatch
set autoindent
set smartindent
set smarttab
set number
set wildmode=longest,list,full
set wildmenu
set ruler
nnoremap <C-t> :tabedit 
nnoremap <C-Left> :tabprevious<CR>
nnoremap <C-Right> :tabnext<CR>
vnoremap p "_dP
set encoding=utf-8

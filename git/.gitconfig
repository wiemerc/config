[user]
	email = constantin.wiemer@gmx.de
	name = Constantin Wiemer
[core]
	excludesfile = ~/.gitignore_global
[alias]
	unstage = reset HEAD --
	discard = checkout --
	co = checkout
	ci = commit
	last = log -1 HEAD
    st = status
    fixup = commit --amend --no-edit
    ll = log --format='format:%Cred%h %Cgreen%ci %Cblue%an %Creset%s'
[credential]
    helper = osxkeychain
